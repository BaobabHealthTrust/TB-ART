      <tr>
          <td>
            <td><button class='blue' onclick="window.parent.location='<%=path%><%=options%>&report_type=<%=report_type%>'" style='margin: 5px,0px,0px; min-width: 450px'><span> <%=report_name%> </span></button></td>
          </td>
      </tr>
    <%end%>
</table>




